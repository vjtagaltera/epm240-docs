    $ wget -np -r <.../usb-stick/>
    $ mv <.../usb-stick> .

    $ mv bootlin.com/pub/conferences/2019/elce/petazzoni-buildroot-tutorial/src/usb-stick .

    $ tree --charset=ascii usb-stick
    usb-stick
    |-- ede-gpio-app
    |   |-- ede-gpio-app.c
    |   |-- index.html
    |   |-- index.html@C=M;O=A
    |   |-- index.html@C=M;O=D
