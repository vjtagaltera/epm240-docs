    |   |   |   |-- index.html
    |   |   |   |-- index.html@C=M;O=A
    |   |   |   |-- index.html@C=M;O=D
    |   |   |   |-- index.html@C=N;O=A
    |   |   |   |-- index.html@C=N;O=D
    |   |   |   |-- index.html@C=S;O=A
