    |   |-- index.html@C=S;O=A
    |   |-- index.html@C=S;O=D
    |   `-- Makefile
    |-- genimage.cfg
    |-- index.html
    |-- index.html@C=M;O=A
    |-- index.html@C=M;O=D
    |-- index.html@C=N;O=A
